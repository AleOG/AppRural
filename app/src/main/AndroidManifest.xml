<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppRural"
        tools:targetApi="31">
        <activity
            android:name=".views.owner.publish.OwnerViewPublishActivity"
            android:exported="false" />
        <activity
            android:name=".views.owner.alta.room.RoomAltaActivity"
            android:exported="false" />
        <activity
            android:name=".views.owner.alta.OwnerViewAltaActivity"
            android:exported="false" />
        <activity
            android:name=".views.owner.OwnerViewActivity"
            android:exported="false" />
        <activity
            android:name=".views.registration.RegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".views.admin.AdminViewActivity"
            android:exported="false" />
        <activity
            android:name=".views.start.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.home.HomeActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
    </application>

</manifest>